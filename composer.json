{
    "name": "teleclient/madelbase",
    "description": "A base project for Telegram userbots using MadelineProto library",
    "type": "project",
    "support": {
        "issues": "https://github.com/teleclient/madelbase/issues"
    },
    "require": {
        "php": ">=7.2",
        "amphp/file": "~0.3",
        "danog/madelineproto": "4.2.21",
        "amphp/dns": "dev-master#aa1892bd as 0.9",
        "amphp/socket": "0.10.12 as 1",
        "amphp/websocket": "dev-master#db2da8c5b3ed22eae37da5ffa10ab3ea8de19342 as 1",
        "amphp/websocket-client": "dev-master#aff808025637bd705672338b4904ad03a4dbdc04 as 1"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/teleclient/madelbase"
        }
    ],
    "minimum-stability": "dev",
    "license": "AGPL-3.0-only",
    "authors": [
        {
            "name": "Tele Client",
            "email": "man@wow.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "teleclient\\madelbase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "teleclient\\madelbase\\tests\\": "test"
        }
    },
    "scripts": {
        "check": [
            "@code-style",
            "@test"
        ],
        "code-style": "php-cs-fixer fix -v --diff",
        "test": "@php -dzend.assertions=1 -dassert.exception=1 ./vendor/bin/phpunit --coverage-text"
    }
}
